<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog id="SubGroupDialog" title="SubGroupDialog">
		<VBox class="sapUiResponsiveMargin">
			
			<Label text="SubGroupText" labelFor="SubGroupText" design="Bold"/>
			<Input  value="{objectPageView>/inputValidation/SubGroupText}" valueLiveUpdate="true" liveChange="onLiveChange"/>
			
		</VBox>
		<buttons>
			<Button type="Emphasized" text="Save" enabled="{objectPageView>/inputValidation/saveButtonEnabled}" press="onSubGroupSave"/>
			<Button text="Close" press=".onCloseSubGroupDialog"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>